{
  "name": "IHWTableAnnotator",
  "version": "0.0.1",
  "description": "IHW Table Annotator",
  "main": "index.js",
  "scripts": {
    "watch": "babel src --watch --out-dir build --presets=@babel/env --plugins=@babel/transform-runtime ",
    "build": "babel src --out-dir build --presets=@babel/env --plugins=@babel/transform-runtime ",
    "gulpWatch": "./node_modules/gulp/bin/gulp.js watch",
    "openapi": "./node_modules/nodemon/bin/nodemon.js ./test/swagger.server.js",
    "db:create:pg": "db-migrate db:create ihw_annotator_test -e postgres",
    "db:up:pg": "db-migrate up:postgres users -e postgres",
    "db:down:pg": "db-migrate down:postgres -e postgres",
    "db:reset:pg": "db-migrate reset:postgres -e postgres",
    "db:create:sqlite": "touch dev.db",
    "db:up:sqlite": "db-migrate up:sqlite users -e sqlite",
    "db:down:sqlite": "db-migrate down:sqlite -e sqlite",
    "db:reset:sqlite": "db-migrate reset:sqlite -e sqlite",
    "test": "jest test --verbose --testPathIgnorePatterns node_modules database ",
    "test_brk": "mocha --inspect-brk testing/tabuliser_testing.js",
    "test:pg": "DB_DRIVER_TYPE=postgres CONFIG_PATH=${PWD}/test/database jest test/database",
    "test:sqlite": "DB_DRIVER_TYPE=sqlite CONFIG_PATH=${PWD}/test/database jest test/database",
    "test:watch": "jest test --verbose --testPathIgnorePatterns node_modules database --watch",
    "test:watch:pg": "DB_DRIVER_TYPE=postgres CONFIG_PATH=${PWD}/test/database jest --watch",
    "test:watch:sqlite": "DB_DRIVER_TYPE=sqlite CONFIG_PATH=${PWD}/test/database jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:rpsoft/tableAnnotator.git"
  },
  "keywords": [
    "analysis"
  ],
  "author": "Jesus Rodriguez Perez",
  "license": "ISC",
  "homepage": "https://github.com/rpsoft/tableAnnotator",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
